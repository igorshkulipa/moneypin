{
  "name": "moneypin",
  "version": "0.0.0",
  "dependencies": {
    "@angular/animations": "^5.2.3",
    "@angular/compiler-cli": "^5.2.3",
    "@angular/forms": "^5.2.3",
    "@angular/platform-server": "^5.2.3"
  },
  "devDependencies": {
    "@angular/common": "^5.2.4",
    "@angular/compiler": "^5.2.4",
    "@angular/core": "^5.2.4",
    "@angular/http": "^5.2.3",
    "@angular/platform-browser": "^5.2.4",
    "@angular/platform-browser-dynamic": "^5.2.4",
    "@angular/router": "^5.2.3",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.10",
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.9.0",
    "bootstrap": "^3.3.6",
    "bower": "^0.10.0",
    "concurrently": "^3.5.1",
    "core-js": "^2.5.3",
    "cpx": "^1.5.0",
    "es6-shim": "^0.35.1",
    "http-server": "^0.11.1",
    "less": "^2.7.3",
    "lodash": "^4.13.1",
    "matchdep": "~0.1.1",
    "npm-watch": "^0.3.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.6",
    "signals": "^1.0.0",
    "system": "^1.0.4",
    "systemjs": "^0.19.47",
    "typescript": "2.3.4",
    "watch": "^1.0.2",
    "zone.js": ">=0.6.23"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "watch": {
    "watch_dev": {
      "patterns": [
        "src", "styles"
      ],
      "extensions": "ts,html,htm,less,json",
      "quiet": false
    }
  },
  "scripts": {
    "dev": "npm-watch",
    "server": "npm run http-server",
    "watch_dev": "npm run build",
    "build": "npm run buildts && npm run buildless && npm run copy",
    "buildts": "tsc",
    "buildless": "lessc styles/less/main.less styles/css/main.css",
    "copy": "npm run copystyles && npm run copyassets",
    "copystyles": "cpx \"styles/**/*.css\" dist/styles",
    "copyassets": "cpx \"src/**/*.{html,htm,png,jpg,jpeg,json}\" dist",
    "http-server": "http-server ./"
  }
}
